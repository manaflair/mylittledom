{
  "name": "@manaflair/mylittledom",
  "private": true,
  "version": "0.4.2",
  "license": "MIT",
  "scripts": {
    "install:dev": "npm-git install --verbose",
    "docs": "npm run build:demo",
    "test": "mocha --compilers js:babel-register -r core-js $(find sources -name '*.test.js')",
    "test:debug": "mocha --debug-brk --inspect --compilers js:babel-register -r core-js $(find sources -name '*.test.js')",
    "demo": "node -r babel-register -r core-js -r ./examples/setup --",
    "demo:debug": "node --debug-brk --inspect -r babel-register -r core-js -- examples/simple-form",
    "build": "babel -D -d dist sources",
    "build:demo": "NODE_ENV=production webpack",
    "serve:demo": "webpack-dev-server",
    "push": "npm run build && cp README.md dist/ && jq 'del(.private)' package.json > dist/package.json && (cd dist && npm publish)"
  },
  "dependencies": {
    "@manaflair/term-strings": "^0.8.1",
    "@manaflair/text-layout": "^0.7.0",
    "core-decorators": "^0.13.0",
